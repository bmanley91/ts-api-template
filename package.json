{
  "name": "ts-api-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:bmanley91/ts-api-template.git",
  "author": "Brian Manley <brian.manley91@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "create-migration": "node_modules/.bin/node-pg-migrate --migration-file-language ts create",
    "migrate": "DATABASE_URL=postgres://postgres:postgres@postgres:5432/ts_api_template node_modules/.bin/node-pg-migrate up",
    "migrate-down": "DATABASE_URL=postgres://postgres:postgres@postgres:5432/ts_api_template node_modules/.bin/node-pg-migrate down"
  },
  "dependencies": {
    "config": "^3.3.9",
    "express": "^4.18.2",
    "node-config": "^0.0.2",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.10.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/node-pg-migrate": "^2.3.1",
    "@types/pg": "^8.6.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
